<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/_46sdp"
    tools:context=".Fragment.MoreFragment">



    <androidx.cardview.widget.CardView
        android:id="@+id/txt_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:cardElevation="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_gravity="end"
        android:layout_marginStart="@dimen/_24sdp"
        android:layout_marginEnd="@dimen/_24sdp"
        app:cardCornerRadius="@dimen/_8sdp"
        xmlns:tools="http://schemas.android.com/tools">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:orientation="horizontal">
            <TextView
                android:layout_width="@dimen/_200sdp"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_18ssp"
                android:fontFamily="@font/tajawal_regular"
                android:padding="@dimen/_8sdp"
                android:layout_marginEnd="@dimen/_32sdp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/profile"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:background="@color/primary"
                android:layout_width="@dimen/_12sdp"
                android:layout_height="match_parent"
                />
        </LinearLayout>




    </androidx.cardview.widget.CardView>





    <androidx.cardview.widget.CardView
        android:id="@+id/txt_palestinTrsadotional"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:cardElevation="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_gravity="end"
        android:layout_marginStart="@dimen/_24sdp"
        android:layout_marginEnd="@dimen/_24sdp"
        app:cardCornerRadius="@dimen/_8sdp"
        xmlns:tools="http://schemas.android.com/tools">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:orientation="horizontal">
            <TextView

                android:layout_width="@dimen/_200sdp"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_18ssp"

                android:fontFamily="@font/tajawal_regular"
                android:layout_marginEnd="@dimen/_32sdp"
                android:padding="@dimen/_8sdp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="تراث فلسطين"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/viewSaved"
                android:background="@color/primary"
                android:layout_width="@dimen/_12sdp"
                android:layout_height="match_parent"
                />
        </LinearLayout>




    </androidx.cardview.widget.CardView>




    <androidx.cardview.widget.CardView
        android:id="@+id/txt_palestinVillages"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:cardElevation="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_gravity="end"
        android:layout_marginStart="@dimen/_24sdp"
        android:layout_marginEnd="@dimen/_24sdp"
        app:cardCornerRadius="@dimen/_8sdp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:orientation="horizontal">



            <TextView
                android:layout_width="@dimen/_200sdp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_32sdp"
                android:padding="@dimen/_8sdp"
                android:textSize="@dimen/_18ssp"

                android:fontFamily="@font/tajawal_regular"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="اشهر القرى الفلسطينية"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:background="@color/primary"
                android:layout_width="@dimen/_12sdp"
                android:layout_height="match_parent"
                />
        </LinearLayout>




    </androidx.cardview.widget.CardView>













    <androidx.cardview.widget.CardView
        android:id="@+id/txt_aboutPalestine"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:cardElevation="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_gravity="end"
        android:layout_marginStart="@dimen/_24sdp"
        android:layout_marginEnd="@dimen/_24sdp"
        app:cardCornerRadius="@dimen/_8sdp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:orientation="horizontal">



            <TextView
                android:text="فلسطين"
                android:layout_width="@dimen/_200sdp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_32sdp"
                android:padding="@dimen/_8sdp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="@dimen/_18ssp"

                android:fontFamily="@font/tajawal_regular"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:background="@color/primary"
                android:layout_width="@dimen/_12sdp"
                android:layout_height="match_parent"
                />
        </LinearLayout>




    </androidx.cardview.widget.CardView>















    <androidx.cardview.widget.CardView
        android:id="@+id/txt_book"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:cardElevation="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_gravity="end"
        android:layout_marginStart="@dimen/_24sdp"
        android:layout_marginEnd="@dimen/_24sdp"
        app:cardCornerRadius="@dimen/_8sdp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:orientation="horizontal">



            <TextView
                android:text="كتب عن فلسطين "
                android:layout_width="@dimen/_200sdp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_32sdp"
                android:padding="@dimen/_8sdp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="@dimen/_18ssp"

                android:fontFamily="@font/tajawal_regular"
                />
            <androidx.appcompat.widget.AppCompatButton
                android:background="@color/primary"
                android:layout_width="@dimen/_12sdp"
                android:layout_height="match_parent"
                />
        </LinearLayout>




    </androidx.cardview.widget.CardView>







    <androidx.cardview.widget.CardView
        android:id="@+id/txet_res"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:cardElevation="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_gravity="end"
        android:layout_marginStart="@dimen/_24sdp"
        android:layout_marginEnd="@dimen/_24sdp"
        app:cardCornerRadius="@dimen/_8sdp">
